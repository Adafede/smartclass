PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title: Which pairs of stereoisomers are present on Wikidata?
SELECT ?structure_1 ?structure_2 WHERE {
    ?structure_1 wdt:P3364 ?structure_2.
}
